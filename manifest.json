{
    "manifest_version": 3,
    "name": "Stock Analysis",
    "version": "1.0",
    "description": "Fetches stock news, financial estimates, and price targets.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    }
}